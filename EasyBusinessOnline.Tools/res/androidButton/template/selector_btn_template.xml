<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Button禁用的背景 -->
    <item android:drawable="@drawable/shape_bg_disabled_template" android:state_enabled="false"></item>
    <!-- Button正常状态下的背景 -->
    <item android:drawable="@drawable/shape_bg_normal_template" android:state_pressed="false"/>
    <!-- Button按下时的背景 -->
    <item android:drawable="@drawable/shape_btn_bg_pressed_template" android:state_pressed="true"/>

</selector>
